#input 1 and cast to int
num1 = int(input("enter number:\n"))

#input 2 and cast to int
num2 = int(input("enter 2nd number:\n"))

#add and print
print("num1 + num2:", num1 + num2)

#subtract and print
print("num1 - num2:", num1 - num2)

#input 3 and cast to int
num3 = int(input("enter 3rd number:\n"))

#input 4 and cast to int
num4 = int(input("enter 4th number:(do not enter 0)\n"))

# multiply and print
print("num3 * num4:", num3 * num4)

# divide and print
print("num3 / num4:", num3 / num4)
